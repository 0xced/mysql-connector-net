{
  "version": "6.10.0-alpha-*",
  "description": "MySql.Data.MySqlClient .Net Core Class Library",
  "authors": [ "Oracle" ],
  "copyright": "Copyright Â© 2016, Oracle and/or its affiliates. All rights reserved.",

  "packOptions": {
    "projectUrl": "http://dev.mysql.com/downloads/",
    "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.html",
    "iconUrl": "http://www.mysql.com/common/logos/logo-mysql-170x115.png",
    "tags": [ "MySql", ".NET Connector", "MySql Connector/NET", "netcore", ".Net Core", "MySql Conector/Net Core", "coreclr", "C/NET", "C/Net Core" ],
    "content": "content/Readme.txt",
    "releaseNotes": "Review Readme.txt for details.",
    "requireLicenseAcceptance": true
  },

  "buildOptions": {
    "define": [ "NETCORE10" ],
    "warningsAsErrors": false,
    "embed": [ "Properties/ReservedWords.txt" ],
    "compile": {
      "exclude": [
        "Framework/Net451/**/*.*",
        "Replication/**/*.*",        
        "zlib/**/*.*"
      ],
      "excludeFiles": [
        "Properties/VersionInfo.cs",
        "CommandBuilder.cs",
        "dataadapter.cs",
        "Installer.cs",
        "MySqlClientPermission.cs",
        "MySqlClientPermissionAttribute.cs",
        "MySqlConfiguration.cs",
        "MySqlPromotableTransaction.cs",
        "MySqlSecurityPermission.cs",
        "SystemPerformanceMonitor.cs",
        "common/NamedPipeStream.cs",
        "common/NativeMethods.cs",
        "common/SHA1.cs",
        "common/SharedMemoryStream.cs",
        "common/SocketStream.cs"
      ]
    },
    "optimize": true,
    "xmlDoc": false,
    "keyFile": "../../ConnectorNetPublicKey.snk",
    "preserveCompilationContext": true,
    "delaySign": true
  },


  "dependencies": {
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-*",
    "NETStandard.Library": "1.5.0-rc2-*",
    "System.Collections.NonGeneric": "4.0.1-rc2-*",
    "System.ComponentModel": "4.0.1-rc2-*",
    "System.ComponentModel.Annotations": "4.1.0-rc2-*",
    "System.ComponentModel.Primitives": "4.0.1-rc2-*",
    "System.ComponentModel.TypeConverter": "4.0.1-rc2-*",
    "System.Data.Common": "4.0.1-rc2-*",
    "System.Data.SqlClient": "4.1.0-rc2-*",
    "System.Diagnostics.Process": "4.1.0-rc2-*",
    "System.Diagnostics.TextWriterTraceListener": "4.0.0-rc2-*",
    "System.IO.Compression": "4.1.0-rc2-*",
    "System.IO.FileSystem.Primitives": "4.0.1-rc2-*",
    "System.Linq.Expressions": "4.0.11-rc2-*",
    "System.Net.NameResolution": "4.0.0-rc2-*",
    "System.Net.Security": "4.0.0-rc2-*",
    "System.Net.Sockets": "4.1.0-rc2-*",
    "System.Reflection": "4.1.0-rc2-*",
    "System.Reflection.TypeExtensions": "4.1.0-rc2-*",
    "System.Security.Principal.Windows": "4.0.0-rc2-*",
    "System.Text.Encoding.CodePages": "4.0.1-rc2-*",
    "System.Threading.Timer": "4.0.1-rc2-*"
  },

  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "DNXCORE50",
        "portable-net452+win81"
      ]
    }
  }
}
