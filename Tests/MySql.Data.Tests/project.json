        "dnxcore50"
      ],
      "buildOptions": {
        "define": [ "NETCORE10" ],
        "copyToOutput": [ "appsettings.json", "client.pfx" ],
        "compile": {
          "include": [ "../*.cs" ],
          "exclude": [
            "**/._*",
            "Framework/Net451/**/*.*"
          ],
          "excludeFiles": [
            "DataTypeTests.cs",
            "DateTimeTests.cs",
            "GetSchemaTests.cs",
            "InstallerTests.cs",
            "MySqlCommandBuilderTests.cs",
            "MySqlDataAdapterTests.cs",
            "MySqlDataReaderTests.cs",
            "MySQLHelperTests.cs",
